{
  "name": "gatsby-source-graphql-universal",
  "description": "Gatsby plugin which adds a third-party GraphQL API to Gatsby GraphQL with client side execution",
  "version": "0.1.3",
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "bugs": {
    "url": "https://github.com/birkir/gatsby-source-graphql-universal/issues"
  },
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "apollo-boost": "^0.1.23",
    "apollo-link": "1.2.1",
    "apollo-link-http": "^1.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^3.0.4",
    "invariant": "^2.2.4",
    "lodash.clonedeep": "4.5.0",
    "node-fetch": "^1.7.3",
    "traverse": "0.6.6",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.1.4"
  },
  "homepage": "https://github.com/birkir/gatsby-source-graphql-universal/#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "universal"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "repository": "https://github.com/birkir/gatsby-source-graphql-universal",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
